h1{font-size:4rem}@media screen and (max-width:998px){h1{font-size:3.5rem}}@media screen and (max-width:767px){h1{font-size:2.5rem}}h2{font-size:2.75rem}@media screen and (max-width:767px){h2{font-size:2.25rem}}h3{font-size:1.67rem}main{background-color:#fff;color:#000;padding-inline:7rem;padding-block:7rem}@media screen and (max-width:1200px){main{padding-inline:2rem}}@media screen and (min-width:calc(1600px + (7rem * 2))){main{padding-inline:calc((100% - 1600px)/2)}}main h3{margin-top:2rem}main a{word-wrap:break-word;color:#eb5e28}